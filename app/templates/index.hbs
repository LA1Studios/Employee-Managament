<div class="container">
  <h3>Employee Managment</h3>
  
  <ul class="nav nav-tabs">
    {{#each model.tabs as |tab|}}
      {{#link-to tab.route tagName='li' }}
        {{#link-to tab.route}}{{tab.name}}{{/link-to}}
      {{/link-to}}
    {{/each}}
  </ul>
  <div class="tab-content">
    <div role="tabpanel" class="tab-pane active">
      {{outlet}}
    </div>
  </div>
</div>
